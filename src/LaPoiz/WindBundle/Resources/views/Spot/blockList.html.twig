{% macro list(spots) %}
			
			{% for spot in spots %}
				<p>
					<span></span><a class="link_object"  href="{{ path('_spot_display', {'id':spot.id}) }}">
						{{ spot.nom }}
					</a></span>
				{% else %}
					<div id="warning">{{ "warning.list.spot.notFound" | trans }}</div>
				</p>
			{% endfor %}
		
{% endmacro %}